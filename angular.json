{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "audtiotools": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/audtiotools",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.json"
            ],
            "styles": [
              "node_modules/flexboxgrid/dist/flexboxgrid.min.css",
              "node_modules/@fortawesome/fontawesome-free/css/all.css",
              "node_modules/animate.css/animate.css",
              "node_modules/hover.css/css/hover.css",
              "src/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/app/ui/scss"
              ]
            },
            "scripts": [
              "node_modules/d3/dist/d3.js",
              "node_modules/midiconvert/build/MidiConvert.js",
              "node_modules/midi-writer-js/build/browser/index.min.js",
              "node_modules/interactjs/dist/interact.min.js",
              "node_modules/sample-player/dist/sample-player.min.js",
              "node_modules/midiutils/src/MIDIUtils.js",
              "node_modules/fuse.js/dist/fuse.min.js",
              "node_modules/toastr/build/toastr.min.js",
              "node_modules/lodash/lodash.min.js",
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/jqueryui/jquery-ui.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
              "node_modules/packery/dist/packery.pkgd.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "serviceWorker": true
            },
            "development": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.development.ts"
                }
              ],
              "sourceMap": true,
              "extractCss": true
            },
            "hmr": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.hmr.ts"
                }
              ],
              "sourceMap": false,
              "extractCss": false
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "audtiotools:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "audtiotools:build:production"
            },
            "development": {
              "browserTarget": "audtiotools:build:development"
            },
            "hmr": {
              "hmr": true,
              "browserTarget": "audtiotools:build:hmr"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "audtiotools:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.json"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "audtiotools-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "audtiotools:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "audtiotools:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "angular-desktop": {
      "root": "projects/angular-desktop",
      "sourceRoot": "projects/angular-desktop/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/angular-desktop/tsconfig.lib.json",
            "project": "projects/angular-desktop/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/angular-desktop/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/angular-desktop/src/test.ts",
            "tsConfig": "projects/angular-desktop/tsconfig.spec.json",
            "karmaConfig": "projects/angular-desktop/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/angular-desktop/tsconfig.lib.json",
              "projects/angular-desktop/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "audtiotools"
}